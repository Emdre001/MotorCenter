@using Motorcenter.UI.Models.Link;

<article>

    <div class=" bg-light">
        @LinkGroup?.Name
    </div>
    <div>

        @foreach (var link in LinkGroup!.LinkOption)
        {
            <LinkComponent LinkOption="link"/>

        }
    </div>



</article>

@code {
    [Parameter] public  LinkGroup? LinkGroup { get; set; }
}
